category:            Control
build-type:          Simple
cabal-version:       >=1.10

executable Eff_Test
  main-is: main.hs
  default-extensions: RankNTypes
                      TypeOperators
                      DeriveDataTypeable
                      GeneralizedNewtypeDeriving
                      DeriveFunctor
                      FlexibleContexts
                      ExistentialQuantification
                      ScopedTypeVariables
                      NoMonomorphismRestriction
  default-language: Haskell2010
  -- other-modules:
  ghc-options:       -Wall -O2
  build-depends:       base               >= 4.5 && < 5
                     , extensible-effects >= 1.1 && < 1.2